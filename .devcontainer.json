{
    "name": "Trademarks System DevEnv",
    "dockerComposeFile": "docker-compose.yml",
    "service": "web",
    "workspaceFolder": "/app",
    "forwardPorts": [9430],
    "remoteUser": "vscode",
    "mounts": [
      "source=${localWorkspaceFolder},target=/workspace/trademarks,type=bind"
    ],
    "customizations": {
      "vscode": {
        "extensions": [
          "KevinRose.vsc-python-indent",
          "ms-python.python",
          "batisteo.vscode-django",
          "ecmel.vscode-html-css",
          "GitHub.remotehub",
          "oderwat.indent-rainbow"
        ]
      }
    }
  }
  